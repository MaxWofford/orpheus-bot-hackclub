{
  "name": "@hackclub/orpheus-bot",
  "author": "Max Wofford <max@hackclub.com>",
  "license": "MIT",
  "scripts": {
    "dev": "babel-node ./src/index.js",
    "start": "node ./build/index.js",
    "fmt": "npx prettier \"{src/**,}/*.js\" --single-quote --no-semi --arrow-parens avoid --trailing-comma es5 --write",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build -s --copy-files"
  },
  "main": "src/index.js",
  "dependencies": {
    "@bugsnag/js": "^6.4.0",
    "@octokit/request": "^5.3.1",
    "airtable": "0.5.9",
    "animals": "0.0.3",
    "botkit": "0.7.4",
    "botkit-storage-redis": "^1.1.0",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.3",
    "chrono-node": "^2.1.8",
    "form-data": "^3.0.0",
    "humanize-duration": "^3.20.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.19",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0"
  }
}
